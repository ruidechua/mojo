module rotate (
  input a[8],
  input alufn[6],
  output rot[8]
  )
  always {
    case(alufn[3:2]) {
    2b01:                 //rotate left
      rot = a<<1;
      rot[0] = a[7];
      
    2b11:                //rotate right
      rot = a>>1;
      rot[7] = a[0];
      
    default: shift = a;
    }
}
